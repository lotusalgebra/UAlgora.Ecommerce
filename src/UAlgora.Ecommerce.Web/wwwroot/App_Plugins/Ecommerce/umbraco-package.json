{
  "$schema": "../../umbraco-package-schema.json",
  "id": "UAlgora.Ecommerce",
  "name": "Algora Commerce",
  "version": "2.8.0",
  "allowTelemetry": true,
  "extensions": [
    {
      "type": "bundle",
      "alias": "UAlgora.Ecommerce.Bundle",
      "name": "Algora Commerce Bundle",
      "js": "/App_Plugins/Ecommerce/index.js"
    },
    {
      "type": "section",
      "alias": "Umb.Section.Ecommerce",
      "name": "Algora",
      "weight": 100,
      "meta": {
        "label": "Algora",
        "pathname": "ecommerce"
      },
      "conditions": [
        {
          "alias": "Umb.Condition.SectionUserPermission",
          "match": "Umb.Section.Ecommerce"
        }
      ]
    },
    {
      "type": "sectionSidebarApp",
      "kind": "tree",
      "alias": "Umb.SectionSidebarApp.Ecommerce.Tree",
      "name": "Algora Tree",
      "element": "/App_Plugins/Ecommerce/views/trees/ecommerce-sidebar-tree.js",
      "weight": 100,
      "meta": {
        "label": "Algora"
      },
      "conditions": [
        {
          "alias": "Umb.Condition.SectionAlias",
          "match": "Umb.Section.Ecommerce"
        }
      ]
    },
    {
      "type": "dashboard",
      "alias": "Umb.Dashboard.Ecommerce",
      "name": "Algora Dashboard",
      "element": "/App_Plugins/Ecommerce/views/dashboard.js",
      "weight": 100,
      "meta": {
        "label": "Dashboard",
        "pathname": "dashboard"
      },
      "conditions": [
        {
          "alias": "Umb.Condition.SectionAlias",
          "match": "Umb.Section.Ecommerce"
        }
      ]
    },
    {
      "type": "sectionView",
      "alias": "Umb.SectionView.Ecommerce.Products",
      "name": "Products View",
      "element": "/App_Plugins/Ecommerce/views/collections/product-collection.js",
      "weight": 200,
      "meta": {
        "label": "Products",
        "pathname": "products",
        "icon": "icon-box"
      },
      "conditions": [
        {
          "alias": "Umb.Condition.SectionAlias",
          "match": "Umb.Section.Ecommerce"
        }
      ]
    },
    {
      "type": "sectionView",
      "alias": "Umb.SectionView.Ecommerce.Categories",
